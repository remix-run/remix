{
  "name": "@mjackson/multipart-parser",
  "version": "0.1.0",
  "description": "A fast, efficient parser for multpart streams",
  "author": "Michael Jackson <mjijackson@gmail.com>",
  "license": "MIT",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "dependencies": {
    "fetch-super-headers": "^0.4.1"
  },
  "devDependencies": {
    "@types/node": "^20.14.10",
    "tsimp": "^2.0.11",
    "typescript": "^5.5.3"
