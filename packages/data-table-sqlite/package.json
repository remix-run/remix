{
  "name": "@remix-run/data-table-sqlite",
  "version": "0.0.0",
  "description": "SQLite adapter for @remix-run/data-table",
  "author": "Michael Jackson <mjijackson@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/remix-run/remix.git",
    "directory": "packages/data-table-sqlite"
  },
  "homepage": "https://github.com/remix-run/remix/tree/main/packages/data-table-sqlite#readme",
  "files": [
    "LICENSE",
    "README.md",
    "dist",
    "src",
    "!src/**/*.test.ts"
  ],
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "devDependencies": {
    "@remix-run/data-schema": "workspace:*",
    "@remix-run/data-table": "workspace:*",
