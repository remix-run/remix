{
  "name": "@remix-run/auth-middleware",
  "version": "0.0.0",
  "description": "Middleware for authenticating users.",
  "author": "Jacob Ebey <jacob.ebey@live.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/remix-run/remix.git",
    "directory": "packages/auth-middleware"
  },
  "homepage": "https://github.com/remix-run/remix/tree/main/packages/auth-middleware#readme",
  "files": [
    "LICENSE",
    "README.md",
    "dist",
    "src",
    "!src/**/*.test.ts"
  ],
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./basic": "./src/basic.ts",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "./basic": {
        "types": "./dist/basic.d.ts",
        "default": "./dist/basic.js"
      },
      "./package.json": "./package.json"
    }
  },
  "devDependencies": {
    "@remix-run/cookie": "workspace:*",
    "@remix-run/fetch-router": "workspace:*",
    "@remix-run/session": "workspace:*",
    "@remix-run/session-middleware": "workspace:*",
    "@types/node": "catalog:",
    "@typescript/native-preview": "catalog:"
  },
  "dependencies": {
    "@remix-run/fetch-router": "workspace:^"
  },
  "scripts": {
    "build": "tsgo -p tsconfig.build.json",
    "clean": "git clean -fdX",
    "prepublishOnly": "pnpm run build",
    "test": "node --disable-warning=ExperimentalWarning --test",
    "typecheck": "tsgo --noEmit"
  },
  "keywords": [
    "fetch",
    "router",
    "middleware",
    "auth",
    "async-local-storage"
  ]
}
