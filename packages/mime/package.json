{
  "name": "@remix-run/mime",
  "version": "0.1.0",
  "description": "Utilities for working with MIME types",
  "author": "Michael Jackson <mjijackson@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/remix-run/remix.git",
    "directory": "packages/mime"
  },
  "homepage": "https://github.com/remix-run/remix/tree/main/packages/mime#readme",
  "files": [
    "LICENSE",
    "README.md",
    "dist",
    "src",
    "!src/**/*.test.ts"
  ],
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "./package.json": "./package.json"
    }
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "@typescript/native-preview": "catalog:",
    "mime-db": "^1.53.0",
    "typescript": "catalog:"
  },
  "scripts": {
    "build": "tsgo -p tsconfig.build.json",
    "clean": "git clean -fdX",
    "codegen": "node --disable-warning=ExperimentalWarning ./scripts/codegen.js",
    "prepublishOnly": "pnpm run build",
    "test": "node --disable-warning=ExperimentalWarning --test",
    "typecheck": "tsgo --noEmit"
  },
  "keywords": [
    "mime",
    "mime-type",
    "media-type",
    "content-type",
    "compressible"
  ]
}
