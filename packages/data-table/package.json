{
  "name": "@remix-run/data-table",
  "version": "0.0.0",
  "description": "A typed, relational query toolkit for Remix",
  "author": "Michael Jackson <mjijackson@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/remix-run/remix.git",
    "directory": "packages/data-table"
  },
  "homepage": "https://github.com/remix-run/remix/tree/main/packages/data-table#readme",
  "files": [
    "LICENSE",
    "README.md",
    "dist",
    "src",
    "!src/**/*.test.ts"
  ],
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": "./src/index.ts",
    "./memory-adapter": "./src/memory-adapter.ts",
    "./operators": "./src/operators.ts",
    "./sql": "./src/sql.ts",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "./memory-adapter": {
        "types": "./dist/memory-adapter.d.ts",
        "default": "./dist/memory-adapter.js"
      },
      "./operators": {
        "types": "./dist/operators.d.ts",
        "default": "./dist/operators.js"
      },
      "./sql": {
        "types": "./dist/sql.d.ts",
        "default": "./dist/sql.js"
      },
      "./package.json": "./package.json"
    }
  },
  "devDependencies": {
    "@remix-run/data-schema": "workspace:*",
    "@types/node": "catalog:",
    "@typescript/native-preview": "catalog:"
  },
  "dependencies": {
    "@remix-run/data-schema": "workspace:^"
  },
  "scripts": {
    "build": "tsgo -p tsconfig.build.json",
    "clean": "git clean -fdX",
    "prepublishOnly": "pnpm run build",
    "test": "node --disable-warning=ExperimentalWarning --test",
    "typecheck": "tsgo --noEmit"
  },
  "keywords": [
    "remix",
    "orm",
    "sql",
    "database",
    "query-builder",
    "relational"
  ]
}
