{
  "name": "@remix-run/session",
  "version": "0.4.1",
  "description": "Session management for JavaScript",
  "author": "Michael Jackson <mjijackson@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/remix-run/remix.git",
    "directory": "packages/session"
  },
  "homepage": "https://github.com/remix-run/remix/tree/main/packages/session#readme",
  "files": [
    "LICENSE",
    "README.md",
    "dist",
    "src",
    "!src/**/*.test.ts"
  ],
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./cookie-storage": "./src/cookie-storage.ts",
    "./fs-storage": "./src/fs-storage.ts",
    "./memory-storage": "./src/memory-storage.ts",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "./cookie-storage": {
        "types": "./dist/cookie-storage.d.ts",
        "default": "./dist/cookie-storage.js"
      },
      "./fs-storage": {
        "types": "./dist/fs-storage.d.ts",
        "default": "./dist/fs-storage.js"
      },
      "./memory-storage": {
        "types": "./dist/memory-storage.d.ts",
        "default": "./dist/memory-storage.js"
      },
      "./package.json": "./package.json"
    }
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "@typescript/native-preview": "catalog:"
  },
  "scripts": {
    "build": "tsgo -p tsconfig.build.json",
    "clean": "git clean -fdX",
    "prepublishOnly": "pnpm run build",
    "test": "node --disable-warning=ExperimentalWarning --test",
    "test:bun": "bun test",
    "typecheck": "tsgo --noEmit"
  },
  "keywords": [
    "session",
    "session-management",
    "session-storage",
    "fetch",
    "cookie",
    "storage"
  ]
}
