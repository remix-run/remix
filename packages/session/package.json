{
  "name": "@remix-run/session",
  "version": "0.1.0",
  "description": "Session management for JavaScript",
  "author": "Michael Jackson <mjijackson@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/remix-run/remix.git",
    "directory": "packages/session"
  },
  "homepage": "https://github.com/remix-run/remix/tree/main/packages/session#readme",
  "files": [
    "LICENSE",
    "README.md",
    "dist",
    "src",
    "!src/**/*.test.ts"
  ],
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./cookie-storage": "./src/cookie-storage.ts",
    "./file-storage": "./src/file-storage.ts",
    "./memory-storage": "./src/memory-storage.ts",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "./cookie-storage": {
        "types": "./dist/cookie-storage.d.ts",
        "default": "./dist/cookie-storage.js"
      },
      "./file-storage": {
        "types": "./dist/file-storage.d.ts",
        "default": "./dist/file-storage.js"
      },
      "./memory-storage": {
        "types": "./dist/memory-storage.d.ts",
        "default": "./dist/memory-storage.js"
      },
      "./package.json": "./package.json"
    }
  },
  "devDependencies": {
    "@types/node": "^24.6.0",
    "typescript": "^5.9.3"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean": "git clean -fdX",
    "prepublishOnly": "pnpm run build",
    "test": "node --disable-warning=ExperimentalWarning --test './src/**/*.test.ts'",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "session",
    "session-management",
    "session-storage",
    "fetch",
    "cookie",
    "storage"
  ]
}
