{
  "name": "@remix-run/method-override-middleware",
  "version": "0.1.1",
  "description": "Middleware for overriding HTTP request methods from form data",
  "author": "Michael Jackson <mjijackson@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/remix-run/remix.git",
    "directory": "packages/method-override-middleware"
  },
  "homepage": "https://github.com/remix-run/remix/tree/main/packages/method-override-middleware#readme",
  "files": [
    "LICENSE",
    "README.md",
    "dist",
    "src",
    "!src/**/*.test.ts"
  ],
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "./package.json": "./package.json"
    }
  },
  "devDependencies": {
    "@remix-run/fetch-router": "workspace:*",
    "@remix-run/form-data-middleware": "workspace:*",
    "@types/node": "^24.6.0",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "@remix-run/fetch-router": "workspace:^"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean": "git clean -fdX",
    "prepublishOnly": "pnpm run build",
    "test": "node --disable-warning=ExperimentalWarning --test './src/**/*.test.ts'",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "fetch",
    "router",
    "middleware",
    "method-override",
    "http-method"
  ]
}
